<!DOCTYPE html>
<html>
<head>
  <style>
    #map {
      height: 600px;
      width: 100%;
    }
  </style>
</head>
<body>
<h3>My Google Maps Demo</h3>
<div id="map"></div>
<script>
  function initMap() {
    var uluru = {lat: 18.37481, lng: -66.06836849999999};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 12,
      center: uluru
    });
    var marker = new google.maps.Marker({
      position: uluru,
      map: map
    });
  }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCFDVUVlg7TioceKeF4lCceOHRxHhjeObQ&callback=initMap">
</script>
</body>
</html>
